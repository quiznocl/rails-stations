<h1>座席表1</h1>

<table>
    <tr>
        <th>1</th>
        <th>2</th>
        <th>3</th>
        <th>4</th>
        <th>5</th>
    </tr>

    <% @sheets.each_slice(5) do |sheet1,sheet2,sheet3,sheet4,sheet5|%>
        <tr>
            <td><%=sheet1.row%>-<%=sheet1.column%></td>
            <td><%=sheet2.row%>-<%=sheet2.column%></td>
            <td><%=sheet3.row%>-<%=sheet3.column%></td>
            <td><%=sheet4.row%>-<%=sheet4.column%></td>
            <td><%=sheet5.row%>-<%=sheet5.column%></td>
        </tr>
    <% end%>
</table>

<div>
    <%= flash[:danger] %>
</div>